{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "incremental": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "jsx": "react-jsx",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "paths": {
      "~/client/*": ["./app/*"]
    },
    "allowImportingTsExtensions": true,
    "noEmit": true
  },
  "include": ["app"]
}